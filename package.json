{
  "name": "jsdrink",
  "version": "0.0.2",
  "description": "Simple parser combinators library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/cordx56/jsdrink"
  },
  "homepage": "https://github.com/cordx56/jsdrink",
  "author": "cordx56 <cordx56@cordx.net>",
  "license": "MIT",
  "keywords": [
    "parser",
    "parser combinator",
    "parser-combinator",
    "parser combinators",
    "parser-combinators"
  ],
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf dist",
    "format": "prettier -w src",
    "format:check": "prettier -c src",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "prepublish": "yarn run build",
    "test": "jest",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.2"
  }
}
